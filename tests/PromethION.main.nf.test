nextflow_pipeline {

    name "Test Workflow main.nf on PromethION data"
    script "../main.nf"
    tag "seqinspector"
    tag "PIPELINE"

    test("PromethION data test") {

        when {
            params {
                outdir = "tests/results/PromethION"
                input  = "https://raw.githubusercontent.com/nf-core/test-datasets/seqinspector/testdata/PromethION/samplesheet.csv"
            }
        }

        then {
            assert workflow.success
        }

    }

}
